<h1>Users</h1>

<table>
  <tr>
    <th>Title</th>
    <th colspan="2"></th>
  </tr>
 

 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.wpass %></td>
      <td><%= user.email %></td>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
    </tr>
  <% end %>
</table>
